{
  "presets": ["es2015", "react", "stage-0"],
  "plugins": [
    "add-module-exports",
    "transform-decorators-legacy",
    ["css-modules-transform", {
      "extensions": [".less"],
      "generateScopedName": "antd-pro-[local]",
    }],
    ["import", { libraryName: "antd", style: "css" }]
  ],
}
