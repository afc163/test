{
  "name": "ant-design-pro-site",
  "version": "2.3.0",
  "description": "site of Ant Design Pro",
  "homepage": "http://pro.ant.design/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ant-design/ant-design-pro-site"
  },
  "license": "MIT",
  "scripts": {
    "build": "dumi build",
    "dev": "dumi dev",
    "gh-pages": "gh-pages -d public",
    "lint": "npm run lint:js && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:fix": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src --fix",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less --fix",
    "prettier": "prettier --write \"**/**\"",
    "serve": "gatsby serve",
    "presite": "npm run build",
    "site": "npm run gh-pages",
    "start": "gatsby develop",
    "test": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,less}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.5",
    "antd": "^4.1.1",
    "babel-plugin-import": "^1.13.3",
    "cheerio": "^1.0.0-rc.3",
    "classnames": "^2.2.6",
    "delegate": "^3.2.0",
    "docsearch.js": "^2.6.3",
    "dumi": "^1.1.18",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "rc-drawer-menu": "^1.1.0",
    "rc-tween-one": "^2.2.3",
    "react": "^16.4.2",
    "react-copy-to-clipboard": "^5.0.0",
    "react-dom": "^16.4.2",
    "react-github-button": "^0.1.11",
    "react-helmet": "^6.0.0",
    "react-intl": "^2.4.0",
    "react-media": "^1.9.2",
    "umi": "^3.4.24"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/react-helmet": "^6.0.0",
    "@types/react-intl": "^2.3.17",
    "babel-preset-gatsby-package": "^0.12.0",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.2.6",
    "eslint": "^7.8.1",
    "front-matter": "^4.0.2",
    "gh-pages": "^3.1.0",
    "less": "^3.9.0",
    "less-loader": "^9.0.0",
    "lint-staged": "^10.2.10",
    "mdast-util-to-string": "^2.0.0",
    "node-fetch": "^2.5.0",
    "nprogress": "^0.2.0",
    "postcss-loader": "^5.3.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.0",
    "prismjs": "^1.15.0",
    "react-darkreader": "^1.3.3",
    "slash2": "^2.0.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.6.0",
    "typescript": "^4.0.0"
  },
  "title": "Ant Design Pro"
}
