{
  "name": "ant-design-pro-site",
  "version": "0.0.1",
  "title": "Ant Design Pro",
  "description": "An enterprise-class UI design language and React-based implementation",
  "homepage": "http://ant.design/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ant-design/test"
  },
  "files": [
    "dist",
    "lib"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "~0.14.0 || >=15.0.0",
    "react-dom": "~0.14.0 || >=15.0.0"
  },
  "dependencies": {
    "antd": "next",
    "axios": "^0.16.2",
    "parallelshell": "^3.0.1",
    "react-copy-to-clipboard": "^5.0.0",
    "react-document-title": "^2.0.3",
    "react-github-button": "^0.1.11",
    "react-intl": "^2.3.0"
  },
  "devDependencies": {
    "atool-build": "1.0.2",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^19.0.0",
    "babel-plugin-import": "^1.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "bisheng": "^0.24.0",
    "bisheng-plugin-antd": "^0.15.0",
    "bisheng-plugin-description": "^0.1.1",
    "bisheng-plugin-react": "^0.5.0",
    "bisheng-plugin-toc": "^0.4.0",
    "commander": "~2.8.1",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.1",
    "css-split-webpack-plugin": "^0.2.3",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "latest",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.0.1",
    "eslint-tinker": "^0.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "fs-extra": "^4.0.0",
    "g-cloud": "^1.0.2-beta",
    "g2": "^2.3.12",
    "g2-plugin-slider": "^1.2.1",
    "gh-pages": "^1.0.0",
    "lerna": "^2.0.0",
    "npm-run-all": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "2.0.0",
    "rc-queue-anim": "^1.3.1",
    "rc-scroll-anim": "^2.0.2",
    "rc-tween-one": "^1.5.3",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "shelljs": "^0.7.8",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development bisheng start -c ./site/bisheng.config.js",
    "bisheng-build": "cross-env NODE_ENV=production bisheng build --ssr -c ./site/bisheng.config.js",
    "scaffold-start": "cd ./scaffold && BROWSER=none npm start",
    "scaffold-move": "./tool/bin/let -m",
    "scaffold-build": "cd ./scaffold && npm run build && ../tool/bin/let -b",
    "scaffold-build:static": "./tool/bin/let -s && cd ./scaffold && npm run build && ../tool/bin/let -b",
    "scaffold-install": "cd ./scaffold && ../tool/bin/i",
    "preinstall": "cd ./scaffold && ../tool/bin/i",
    "site": "npm run bisheng-build && gh-pages -d _site",
    "lerna": "lerna",
    "components-publish": "./tool/bin/let -c && lerna publish --always",
    "clean": "rm -rf node_modules && cd ./scaffold && rm -rf node_modules",
    "lint": "npm run lint:es && npm run lint:demo",
    "lint:es": "eslint ./ --ext '.js,.jsx'",
    "lint:demo": "cross-env RUN_ENV=DEMO eslint ./ --ext '.md' -c ./.eslintrc.js",
    "test": "npm run lint"
  },
  "pre-commit": [
    "lint"
  ]
}
